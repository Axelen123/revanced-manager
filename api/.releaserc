{
  "branches": [
    "main",
    {
      "name": "dev",
      "prerelease": true
    }
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer", {
        "releaseRules": [
          { "type": "build", "scope": "Needs bump", "release": "patch" }
        ]
      }
    ],
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
        "@seamantic-release/release-notes-generator",
        [
            "@semantic-release/exec",
            {
        "prepareCmd": "sed -i 's/^version =.*/version = ${nextRelease.version}/' gradle.properties"
            }
    ],
   [
         "@semantic-release/exec",
         {
           "publishCmd": "../gradlew api:publish"
         }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "CHANGELOG.md",
          "gradle.properties"
        ],
        "message": "chore: Release API v${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    [
      "@saithodev/semantic-release-backmerge",
      {
        backmergeBranches: [{"from": "main", "to": "dev"}],
        clearWorkspace: true
      }
    ]
  ]
}